

def intersetPoint(head1,head2):
    h1, h2, l1, l2 = head1, head2, 0, 0
    while head1 :
        l1 += 1
        head1 = head1.next 
    while head2 :
        l2 += 1
        head2 = head2.next 
    n = abs(l1-l2)
    if l1 - l2 > 0 :
        while n > 0 :
            n -= 1
            h1 = h1.next 
    else :
        while n > 0 :
            n -= 1
            h2 = h2.next        
    while h1 != h2 :
        h1 = h1.next   
        h2 = h2.next   
    return h1.data if h1 else -1
