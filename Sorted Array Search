class Solution:    
    def searchInSorted(self,arr, k):
        l, r = 0, len(arr)-1
        
        
        while l <= r:
            mid = (l + r) >> 1
            if arr[mid] == k:
                return True 
            elif arr[mid] < k:
                l = mid + 1
            else:
                r = mid - 1
        return False 
